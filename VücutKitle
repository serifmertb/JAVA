import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        double kg, boy, vki;

        System.out.print("Kg: ");
        kg = sc.nextDouble();

        System.out.print("Boy: ");
        boy = sc.nextDouble();

        vki = kg / (boy * boy);

        System.out.print("Vücut Kitle İndeksi: " + vki );

    }
}
